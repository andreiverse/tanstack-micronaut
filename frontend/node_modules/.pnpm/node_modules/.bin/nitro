#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules/nitro/dist/cli/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules/nitro/dist/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules/nitro/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules/nitro/dist/cli/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules/nitro/dist/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules/nitro/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/nitro@3.0.1-alpha.1_lru-cache@11.2.4_rollup@4.53.4_vite@7.3.0_@types+node@22.19.3_jiti@_5b2506bd70689772c9ca5f9e46c70303/node_modules:/home/d4rck/tanstack-micronaut/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nitro/dist/cli/index.mjs" "$@"
else
  exec node  "$basedir/../nitro/dist/cli/index.mjs" "$@"
fi
